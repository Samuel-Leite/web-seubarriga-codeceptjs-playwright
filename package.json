{
  "name": "seu_barriga",
  "version": "1.0.0",
  "description": "Projeto de automacao utilizando JS atraves da plataforma Seu Barriga",
  "main": "codecept.conf.js",
  "scripts": {
    "prepare": "husky install",
    "precommit": "git add . ",
    "commit": "git cz && node .husky/push.js",
    "lint": "eslint . --max-warnings=0",
    "tag": "npx codeceptjs run ./tests/e2e/*_test.js --verbose --grep",
    "wip": "npx codeceptjs run ./tests/e2e/*_test.js --verbose"
  },
  "lint-staged": {
    "*": [
      "npx lint --fix"
    ]
  },
  "author": "Samuel Leite",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.4.5",
    "eslint-config-airbnb-base": "^15.0.0",
    "js-yaml": "^4.1.0",
    "prettier": "^3.2.5"
  },
  "devDependencies": {
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "git-cz": "^4.9.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2"
  }
}
